{   
    "name": "Website Time Analyzer",
    "description": "Track time spent and analyze on websites",
    "version" : "1",
    "manifest_version": 2,

    "permissions" : [
        "tabs",
        "activeTab",
        "storage",
        "idle",
        "webNaviagation",
        "unlimitesStorage"
    ],

    "offline_enabled" : true,

    "browser_action" : {
        "default_popup" : "index.html",
        "default_title" : "Website Time Analyzer"
    }
}